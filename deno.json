{
  "tasks": {
    "build": "deno run -A build.ts"
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.41.1",
    "@std/assert": "jsr:@std/assert@^0.223.0",
    "jasone": "./src/mod.ts"
  },
  "test": {
    "include": ["test/**/*.ts"]
  },
  "bench": {
    "include": ["benchmark/**/*.ts"]
  },
